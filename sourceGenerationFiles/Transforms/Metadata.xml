	<remove-node path="/api/*/*/field[contains(@name, 'zz')]" />
	<remove-node path="/api/package[@name='com.google.android.gms.internal']" />
    <remove-node path="/api/package[@name='com.google.android.gms.internal.firebase_abt']" />
</metadata>