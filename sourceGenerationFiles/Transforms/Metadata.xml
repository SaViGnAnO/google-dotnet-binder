	<!-- Remove *zz* obfuscated classes, interfaces, methods, etc. -->
	<remove-node path="/api/package/class[contains(@name, 'zz')]" />
	<remove-node path="/api/package/interface[contains(@name, 'zz') and @name != 'zzanb']" />
